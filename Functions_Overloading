#include <iostream>
#include <string>

using namespace std;

void Print(string str);
void Print(int Num);
void Print(double d);

int main()

{
	Print("heelloo");

	system("pause");
}

void Print( int Num) 
{
	cout << Num << endl;
}

void Print(string str) 
{
	cout << str << endl;
}

void Print(double d) 
{
	cout << d << endl;
}
